<%-    include('./partials/header.ejs')                    %>
<body>
    <%- include ('./partials/nav.ejs')%>
    <h1> G2 Test User Information</h1>
    <form id="userForm" action="/G2" method="post">
        <fieldset>
            <%= message %>
            <legend>Personal Information</legend>
            <label for="firstName">First Name:</label>
            <input type="text" id="firstName" name="firstName" required >
            <br>
            <label for="lastName">Last Name:</label>
            <input type="text" id="lastName" name="lastName"  required>
            <br>
            <label for="licenseNumber">License Number:</label>
            <input type="text" id="licenseNo" name="licenseNo" >
            <br>
            <label for="age">Age:</label>
            <input type="number" id="age" name="age" min="1" max="99"  required>
            <br>
            <label for="dob">Date of Birth:</label>
            <input type="date" id="dob" name="dob" required>
        </fieldset>
        <fieldset>
            <legend>Car Information</legend>
            <label for="make">Make:</label>
            <input type="text" id="make" name="make" required>
            <br>
            <label for="model">Model:</label>
            <input type="text" id="model" name="model" >
            <br>
            <label for="year">Year:</label>
            <input type="number" id="year" name="year" min="1900" max="2022" required>
            <br>
            <label for="plate Number">Plate Number:</label>
            <input type="text" id="platno" name="platno" required>
        </fieldset>
        <button type="submit">Submit</button>
    </form>
    <script src="/js/G2_Test.js"></script>
    <%-  include('./partials/footer.ejs')  %> 
</body>
</html>